{
  "name": "zum-portal-core",
  "version": "1.1.0-beta4",
  "description": "Express.js 백엔드 및 Vue 2.6으로 만들어진 코어 프로젝트",
  "author": "partjun <partjun@zuminternet.com>, junil <junil@zuminternet.com>",
  "scripts": {
    "example:backend:dev": "tsc-watch --onSuccess \"cross-env NODE_ENV=development BASE_PATH=.examples nodemon --exec ts-node --transpile-only .examples/backend/Server.ts\"",
    "example:backend-vanilla:dev": "tsc-watch --onSuccess \"cross-env NODE_ENV=development BASE_PATH=.examples nodemon --exec ts-node --transpile-only .examples/backend_vanilla/app.js\"",
    "example:backend:prod": "cross-env ZUM_BACK_MODE=deploy NODE_ENV=production BASE_PATH=.examples ts-node ./.examples/backend/Server.ts",
    "example:frontend:publish": "cross-env ZUM_FRONT_MODE=publish BASE_PATH=.examples vue-cli-service serve",
    "example:frontend:dev": "cross-env ZUM_FRONT_MODE=dev BASE_PATH=.examples vue-cli-service serve",
    "example:frontend:build": "cross-env BASE_PATH=.examples vue-cli-service build --no-clean && cross-env ZUM_FRONT_MODE=ssr BASE_PATH=.examples vue-cli-service build --no-clean",
    "build": "tsc"
  },
  "files": [
    "backend/**",
    "frontend/**",
    "types/**",
    "banner.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.zuminternet.com/zum-portal-framework/zum-portal-core-js-project.git"
  },
  "engines": {
    "node": ">= 8.10.59"
  },
  "publishConfig": {
    "registry": "http://ci-portal.zuminternet.com/nexus/repository/zum-portal-core-js/"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.4",
    "core-js": "~3.6.5",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.2.2",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "fetch-jsonp": "^1.1.3",
    "js-cookie": "^2.2.1",
    "jsdom": "8.5.0",
    "lodash-es": "^4.17.15",
    "morgan": "^1.9.1",
    "node-cache": "^5.1.2",
    "node-sass": "^4.14.1",
    "node-schedule": "^1.3.2",
    "node-yaml-config": "^0.0.5",
    "nodemon": "^2.0.7",
    "reflect-metadata": "^0.1.13",
    "reset-css": "^4.0.1",
    "rimraf": "^3.0.0",
    "tsyringe": "^4.4.0",
    "vuex": "~3.6.0",
    "cors": "^2.8.5",
    "http-proxy-middleware": "^1.0.6",
    "@types/express": "^4.17.11",
    "@types/js-cookie": "^2.2.6",
    "@types/node": "^8.10.59",
    "sass-loader": "~7.3.1",
    "terser-webpack-plugin": "~2.3.2",
    "ts-node": "^9.1.1",
    "vue": "~2.6.12",
    "vue-loader": "~15.9.6",
    "vue-router": "~3.4.9",
    "vue-server-renderer": "~2.6.12",
    "vue-template-compiler": "~2.6.12",
    "typescript": "^4.1.3",
    "@vue/cli-service": "~3.12.1",
    "@vue/cli-plugin-typescript": "~4.1.1",
    "@vue/cli-plugin-babel": "~4.1.2",
    "tsc-watch": "^4.2.9",
    "@sentry/node": "^6.0.2"
  }
}
