{
  "name": "zum-portal-core",
  "version": "2.0.0-beta",
  "description": "NestJS 백엔드 및 Vue 2.6으로 만들어진 코어 프로젝트",
  "author": "partjun <partjun@zuminternet.com>, junil <junil@zuminternet.com>",
  "scripts": {
    "example:backend:prebuild": "rimraf .examples/dist",
    "example:backend:build": "cross-env BASE_PATH=.examples nest build",
    "example:backend:start": "cross-env BASE_PATH=.examples NODE_ENV=production nest start",
    "example:backend:start:dev": "cross-env BASE_PATH=.examples NODE_ENV=developmoent nest start --watch",
    "example:backend:start:debug": "cross-env BASE_PATH=.examples nest start --debug --watch",
    "example:backend:start:prod": "cross-env BASE_PATH=.examples NODE_ENV=production node .examples/dist/.examples/backend/main.js",
    "example:frontend:publish": "cross-env ZUM_FRONT_MODE=publish BASE_PATH=.examples vue-cli-service serve",
    "example:frontend:dev": "cross-env ZUM_FRONT_MODE=dev BASE_PATH=.examples vue-cli-service serve",
    "example:frontend:build": "cross-env BASE_PATH=.examples vue-cli-service build --no-clean && cross-env ZUM_FRONT_MODE=ssr BASE_PATH=.examples vue-cli-service build --no-clean",
    "build": "tsc"
  },
  "files": [
    "backend/**",
    "frontend/**",
    "types/**",
    "banner.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://git.zuminternet.com/zum-portal-framework/zum-portal-core-js-project.git"
  },
  "engines": {
    "node": ">= 8.10.59"
  },
  "publishConfig": {
    "registry": "http://ci-portal.zuminternet.com/nexus/repository/zum-portal-core-js/"
  },
  "dependencies": {
    "@nestjs/cli": "~8.0.0",
    "@nestjs/common": "~8.0.0",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "~8.0.0",
    "@nestjs/platform-express": "~8.0.0",
    "@nestjs/schedule": "^1.0.0",
    "@nestjs/schematics": "~8.0.0",
    "@nestjs/testing": "~8.0.0",
    "@types/cache-manager": "^3.4.2",
    "@types/cron": "^1.7.3",
    "@types/express": "~4.17.13",
    "@types/jest": "~26.0.24",
    "@types/js-cookie": "~2.2.6",
    "@types/node": "~16.0.0",
    "@types/supertest": "~2.0.11",
    "@vue/cli-plugin-babel": "~4.1.2",
    "@vue/cli-plugin-typescript": "~4.1.1",
    "@vue/cli-service": "~3.12.1",
    "axios": "~0.21.1",
    "cache-manager": "^3.4.4",
    "concurrently": "~5.3.0",
    "cookie-parser": "~1.4.4",
    "core-js": "~3.6.5",
    "cross-env": "~7.0.3",
    "deepmerge": "~4.2.2",
    "ejs": "~3.1.5",
    "express": "~4.17.1",
    "fetch-jsonp": "~1.1.3",
    "http-proxy-middleware": "~1.0.6",
    "jest": "~27.0.6",
    "js-cookie": "~2.2.1",
    "jsdom": "8.5.0",
    "node-sass": "~4.14.1",
    "node-yaml-config": "~0.0.5",
    "reflect-metadata": "~0.1.13",
    "reset-css": "~4.0.1",
    "rimraf": "~3.0.2",
    "rxjs": "~7.2.0",
    "sass-loader": "~7.3.1",
    "supertest": "~6.1.3",
    "terser-webpack-plugin": "~2.3.2",
    "ts-jest": "~27.0.3",
    "ts-loader": "~9.2.3",
    "ts-node": "^10.0.0",
    "tsc-watch": "~4.2.9",
    "tsconfig-paths": "~3.10.1",
    "typescript": "~4.3.5",
    "vue": "~2.6.13",
    "vue-loader": "~15.9.6",
    "vue-router": "~3.4.9",
    "vue-server-renderer": "~2.6.13",
